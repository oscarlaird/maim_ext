{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "version": "1.0",
  "permissions": ["storage", "activeTab", "scripting", "sidePanel"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
  ]
}
